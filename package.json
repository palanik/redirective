{
  "name": "redirective",
  "version": "1.0.0",
  "description": "Node.js Express redirect shortcut",
  "main": "lib/index.js",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb/base"
  },
  "scripts": {
    "lint": "eslint src/**",
    "build": "babel --presets es2015 src -d lib",
    "prepublish": "npm run build",
    "start": "node lib/",
    "dev": "./node_modules/.bin/babel-node src/",
    "test-cov": "babel-node ./node_modules/.bin/isparta cover _mocha",
    "test": "babel-node ./node_modules/.bin/_mocha"
  },
  "engines": {
    "node": ">=6.1.0",
    "npm": ">=3.8.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/palanik/redirective.git"
  },
  "keywords": [
   "express",
   "connect",
   "routing",
   "middleware",
   "redirect",
   "redirection",
   "redirector",
   "redirective",
   "301"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "author": "N. Palani Kumanan",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-preset-es2015": "^6.22.0",
    "chai": "^3.5.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "isparta": "^4.0.0",
    "mocha": "^3.2.0",
    "node-mocks-http": "^1.5.6"
  }
}
